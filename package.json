  "keywords": [
    "fabrication",
    "3d printing",
    "laser cutting"
  ],
  "homepage": "http://lowfab.net",
  "bugs": "https://bitbucket.org/hpihci/fab3d-lowfab/issues",
  "main": "build/server/main.js",
  "dependencies": {
    "MD5": "^1.2.1",
    "body-parser": "^1.9.1",
    "bower": "^1.3.12",
    "browserify": "^6.1.0",
    "browserify-data": "^0.2.0",
    "coffee-script": "^1.8.0",
    "coffeeify": "^0.7.0",
    "compression": "^1.2.0",
    "errorhandler": "^1.2.2",
    "exec": "^0.1.2",
    "express": "^4.9.8",
    "express-session": "^1.9.1",
    "jade": "^1.7.0",
    "js-yaml": "^3.2.3",
    "jsondiffpatch": "^0.1.17",
    "mkdirp": "^0.5.0",
    "morgan": "^1.4.1",
    "nib": "^1.0.4",
    "readdirp": "^1.1.0",
    "serve-favicon": "^2.1.6",
    "stylus": "^0.49.2",
    "win-spawn": "^2.0.0",
    "winston": "^0.8.1"
  },
  "devDependencies": {
    "chai": "^1.9.2",
    "coffeelint": "^1.6.1",
    "mocha": "^2.0.1",
    "crojsdoc": "^0.8.8",
    "groc": "^0.7.0"
  },
  "scripts": {
    "build": "./node_modules/.bin/cake build",
    "clean": "./node_modules/.bin/cake clean",
    "start": "./node_modules/.bin/cake start",
    "linkHooks": "./node_modules/.bin/cake linkHooks",
    "checkStyle": "./node_modules/.bin/coffeelint Cakefile .",
    "apiDocumentation": "./node_modules/.bin/crojsdoc -o apidoc src/",
    "documentation": "./node_modules/.bin/groc",
    "test": "./node_modules/.bin/mocha --compilers coffee:coffee-script/register",
    "prepublish": "npm test && npm run checkStyle"
  },
  "repository": {
